 \r\n\r\n  }, [])\r\n\r\n  function ButtonCallback(val:any){\r\n    dispatch({type:val})\r\n  }\r\n\r\n  function edit (id:string) {\r\n    props.history.push(`/project-add?id=${id}`)\r\n  }\r\n  async function del (id:string) {\r\n    const {data} = await delProject(id)\r\n    setRefresh(refresh+1)\r\n    setPage({ current: 1, total: 0 })\r\n    message.success(data.message)\r\n  }\r\n\r\n  const tableColumns = columns.map(col => {\r\n    if (col.dataIndex === 'operation') {\r\n      return { ...col, onCell: (record:any, rowIndex: number) => ({col, record, edit, del, rowIndex }) }\r\n    } else {\r\n      return col\r\n    }\r\n  })\r\n  const components = { body: { cell: EditCell } }\r\n  return <PageLayout title='项目管理'>\r\n    <Upload name='测试文件'/>\r\n    <Space>\r\n      useReducer:{state.count}\r\n      <Button1 Callback={ButtonCallback}/>\r\n      <Button2 Callback={ButtonCallback}/>\r\n    </Space>\r\n\r\n    {/* Context跨组件传值 */}\r\n    <Context.Provider value={state}>\r\n      <App/>\r\n    </Context.Provider>\r\n    \r\n    <Table components={components}\r\n      columns={tableColumns as any}\r\n      dataSource={tableData}\r\n      bordered\r\n      size='middle'\r\n      rowKey='_id'\r\n      pagination={page} />\r\n  </PageLayout>\r\n}\r\n\r\nexport default Projects\r\n","E:\\project\\naice-blog-admin\\src\\pages\\messages-list\\index.tsx",[],"E:\\project\\naice-blog-admin\\src\\utils\\api.ts",[],"E:\\project\\naice-blog-admin\\src\\pages\\article-draft\\article-config.tsx",[],["240","241"],"E:\\project\\naice-blog-admin\\src\\pages\\project-add\\add.tsx",["242","243","244","245","246"],"E:\\project\\naice-blog-admin\\src\\pages\\tags\\article-config.tsx",[],"E:\\project\\naice-blog-admin\\src\\utils\\http.ts",["247","248"],"E:\\project\\naice-blog-admin\\src\\common\\components\\header-title\\index.tsx",[],"E:\\project\\naice-blog-admin\\src\\common\\layout\\menu-config.ts",["249","250","251","252","253"],"import {\r\n  HomeOutlined,\r\n  EditOutlined,\